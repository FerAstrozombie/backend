<%if (productos.length > 0) { %>
    <h2>Productos cargados</h2>
    <table>
        <tr>
            <th>Nombre</strong></th>
            <th>Precio</strong></th>
            <th>Foto</strong></th>
        </tr>
        <% productos.forEach((producto) => { %> 
            <tr>
                <td> <%= producto.title %> </td>
                <td> <%= producto.price %>$ </td>
                <td> <img src=" <%= producto.url %> " alt=" <%= producto.title %> "></td>
            </tr>
        <% }) %> 
    </table>
<% } else { %>
    <h2 class="noHay">No hay productos</h2>
<% } %>